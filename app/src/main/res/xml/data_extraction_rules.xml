<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="file" path="." />
        <exclude domain="database" path="sensitive.db" />
        <exclude domain="sharedpref" path="credentials.xml" />
        <exclude domain="external" path="DinoAdmin/Secret/" />
    </cloud-backup>
    <device-transfer>
        <include domain="file" path="." />
        <exclude domain="database" path="sensitive.db" />
        <exclude domain="sharedpref" path="credentials.xml" />
        <exclude domain="external" path="DinoAdmin/Secret/" />
    </device-transfer>
</data-extraction-rules>
